<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Eventos de Materia para la Fecha</title>
</head>
<body>
  <h1>Eventos de Materia para la Fecha</h1>

  <h2>Materia ID: <%= materiaId %></h2>
  <h2>Fecha: <%= fecha %></h2>

  <% if (eventos && eventos.length > 0) { %>
    <table>
      <thead>
        <tr>
          <th>ID_EVENTO</th>
          <th>TIPO</th>
          <th>FECHA</th>
          <th>MATERIA_ID</th>
          <th>SECCION_ID</th>
          <!-- Agrega más columnas según tus necesidades -->
        </tr>
      </thead>
      <tbody>
        <% eventos.forEach(evento => { %>
          <tr>
            <td><%= evento.ID_EVENTO %></td>
            <td><%= evento.TIPO %></td>
            <td><%= evento.FECHA %></td>
            <td><%= evento.MATERIA_ID %></td>
            <td><%= evento.SECCION_ID %></td>
            <!-- Agrega más celdas según tus necesidades -->
          </tr>
        <% }); %>
      </tbody>
    </table>
  <% } else { %>
    <p>No hay eventos disponibles para esta materia y fecha.</p>
  <% } %>

</body>
</html>